<style type="text/css">
    /*Dashboard Tiles*/
    .grid {
        /* Grid Fallback */
        display: flex;
        flex-wrap: wrap;
        /* Supports Grid */
        display: grid;
        grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
        grid-auto-rows: minmax(150px, auto);
        grid-gap: 1em;
    }

    .module {
        /* Demo-Specific Styles */
        display: inherit;
        align-items: center;
        justify-content: center;
        height: 160px;
        /* Flex Fallback */
        margin-left: 5px;
        margin-right: 5px;
        flex: 1 1 200px;
        color: white !important;
        padding: 35px 10px;
        text-align: center;
    }

        .module a {
            color: white !important;
        }
    /* If Grid is supported, remove the margin we set for the fallback */
    @@supports (display: grid) {
        .module {
            margin: 0;
        }
    }
</style>

