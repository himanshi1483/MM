                        <li style="background-color:#F03A47"><a href="#">View Preliminary List</a></li>
                        <li style="background-color:#276FBF"><a href="#">Preliminary Scrutiny</a></li>
                        <li style="background-color:#183059"><a href="#">View Final List</a></li>
                    }
                    <li style="background-color:#716D77"><a href="@Url.Action("ResetPwdNew", "Menu" , new { username = Model.Username})">Change Password</a></li>
