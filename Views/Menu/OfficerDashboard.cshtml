@model Mahamesh.Models.OfficerLogin

@{
    ViewBag.Title = "Officer Dashboard";
}
<style>
    .lbl {
        top: 8px;
        font-size: 15px;
        text-align: center;
    }

    .table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
        font-size: 15px;
        padding: 4.5px;
    }

    .vertical-nav {
        height: auto;
        list-style: none;
        width: 80%; /******* MODIFIED ********/
        /*margin: 20px 0 0 0;*/
    }

        .vertical-nav li {
            height: 42px; /*//old 30px//*/
            background-color: #46cb92;
            border: 1px solid #fff;
            text-align: left;
            display: inline-block;
            float: left;
            padding-left: 7px;
            padding-top: 10px; /*old 5px*/
            clear: both;
            /*min-width: 282px;*/
            width: 100%;
            /******* MODIFIED ********/
        }

            .vertical-nav li:hover {
                background-color: #fe5719;
                color: #FFFFFF;
            }

            .vertical-nav li a {
                /*font-size: 14px;*/
                color: #ffffff;
            }

            .vertical-nav li.current {
                background-color: #97d2fe;
            }

                .vertical-nav li.current a {
                    color: #FFFFFF;
                }

    vertical-nav ul li ul {
        display: none;
        list-style-type: none;
        /*min-width: 250px;*/
        padding: 0px;
        margin-top: 3px;
        margin-left: 0px;
    }

    vertical-nav ul li:hover ul {
        display: block;
    }

        vertical-nav ul li:hover ul li {
            background-color: #74a9ea;
            /*min-width: 250px;

        min-height: 30px;*/
            display: inline-block;
        }

    vertical-nav ul li ul li:hover {
        background-color: #333333;
    }

    vertical-nav ul li ul li a {
        color: #FFF;
        text-decoration: underline;
    }

        vertical-nav ul li ul li a:hover {
            text-decoration: none;
        }

    .vertical-nav li ul {
        display: none;
        margin-top: -23px;
        padding: 0;
    }

    .vertical-nav li:hover ul {
        display: block;
    }

    .vertical-nav li:hover .sub-menu {
        position: relative;
        margin-top: -27.5px;
    }

    .sub-menu li {
        position: relative;
        display: block;
        top: 0;
        left: 100%;
        width: 100%;
        background-color: #ebb046;
        /*min-width: 180px;
        white-space: nowrap;*/
        z-index: 1;
    }

        .sub-menu li a {
            display: inline-block;
            padding: 0 10px;
        }

        .sub-menu li:hover {
            background-color: #fe5719;
            border: 1px solid #fff;
        }

    .sub-menu1 li {
        position: relative;
        display: block;
        top: 0;
        right: 103%;
        width: 100%;
        background-color: #78c3fb;
        /*min-width: 180px;
        white-space: nowrap;*/
        z-index: 1;
    }

        .sub-menu1 li a {
            display: inline-block;
            padding: 0 10px;
        }

        .sub-menu1 li:hover {
            background-color: #fe5719;
            border: 1px solid #fff;
        }

    @@media screen and (max-width: 700px) {
        .vertical-nav {
            width: 100%;
            height: auto;
            position: relative;
        }

            .vertical-nav li {
                width: 100%;
                height: auto;
                position: relative;
            }
    }

    /* On screens that are less than 400px, display the bar vertically, instead of horizontally */
    @@media screen and (max-width: 400px) {
        .vertical-nav li {
            width: 100%;
            height: auto;
            position: relative;
        }
    }
     .table-striped3 > tbody > tr:nth-of-type(odd) {
