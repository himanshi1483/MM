<div class="container-fluid body-content" id="#maincontent" style="margin-top:10px; margin-bottom: 150px;">
    <div class="panel panel-default" style="box-shadow: 0 2px 2px rgba(0,0,0,.3);">
        <div class="panel-heading panel-default text-center">
            @if (User.IsInRole("DDC"))
            {
                <h4 class="text-center">Welcome DDC</h4>
            }
            else if (User.IsInRole("DAHO"))
