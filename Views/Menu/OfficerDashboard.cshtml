        </div>
        <div class="panel-body">
            <div class="col-md-2">
                <ul class="vertical-nav">
                    @if (User.IsInRole("DDC"))
                    {
                        <li style="background-color:#F03A47"><a id="link1" href="#">View Login Details</a></li>
                    }

                    <li  style="background-color:#3C3744"><a id="link2" href="#">View Targets</a></li>
                    @if (User.IsInRole("DDC"))
                    {
                        <li style="background-color:#276FBF"><a href="@Url.Action("GenerateRandomList", "ApplicantRegistrations")">View Application Statistics</a></li>
                        <li style="background-color:#183059"><a href="#">Generate Random List</a></li>
                        <li style="background-color:#C46D73"><a href="#">Preliminary Document Scrutiny</a></li>
                        <li style="background-color:#0294A5"><a href="#">Final Beneficiary List</a></li>
                    }
                    @if (User.IsInRole("DAHO") || User.IsInRole("LDO"))
