                    color: #FFFFFF;
                }

    vertical-nav ul li ul {
        display: none;
        list-style-type: none;
        /*min-width: 250px;*/
        padding: 0px;
        margin-top: 3px;
        margin-left: 0px;
    }

    vertical-nav ul li:hover ul {
        display: block;
    }

        vertical-nav ul li:hover ul li {
            background-color: #74a9ea;
            /*min-width: 250px;

        min-height: 30px;*/
            display: inline-block;
        }

    vertical-nav ul li ul li:hover {
        background-color: #333333;
    }

    vertical-nav ul li ul li a {
        color: #FFF;
        text-decoration: underline;
    }

        vertical-nav ul li ul li a:hover {
            text-decoration: none;
        }

    .vertical-nav li ul {
        display: none;
        margin-top: -23px;
        padding: 0;
    }

    .vertical-nav li:hover ul {
        display: block;
    }

    .vertical-nav li:hover .sub-menu {
        position: relative;
        margin-top: -27.5px;
    }

    .sub-menu li {
        position: relative;
        display: block;
        top: 0;
        left: 100%;
        width: 100%;
        background-color: #ebb046;
        /*min-width: 180px;
        white-space: nowrap;*/
        z-index: 1;
    }

        .sub-menu li a {
            display: inline-block;
            padding: 0 10px;
        }

        .sub-menu li:hover {
            background-color: #fe5719;
            border: 1px solid #fff;
        }

    .sub-menu1 li {
