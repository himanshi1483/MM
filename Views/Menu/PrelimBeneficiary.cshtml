@model Mahamesh.Models.OfficerLogin


@{
    ViewBag.Title = "Beneficiary List Preliminary & Final";
}
<style>
    .grid {
        /* Grid Fallback */
        display: flex;
        flex-wrap: wrap;
        /* Supports Grid */
        display: grid;
        grid-template-columns: repeat(auto-fill, minmax(375px, 1fr));
        grid-auto-rows: minmax(150px, auto);
        grid-gap: 1em;
    }

    .panel-title {
        font-family: Arial Unicode MS;
        font-weight: 800;
    }

    .module {
        /* Demo-Specific Styles */
        display: inherit;
        align-items: center;
        justify-content: center;
        height: 160px;
        /* Flex Fallback */
        margin-left: 5px;
        margin-right: 5px;
        flex: 1 1 200px;
        color: black !important;
        padding: 35px 10px;
        text-align: center;
    }

        .module a {
            color: black !important;
            font-weight: 600;
        }
    /* If Grid is supported, remove the margin we set for the fallback */
    @@supports (display: grid) {
        .module {
            margin: 0;
        }
    }
</style>
<div class="container body-content" id="#maincontent" style="margin-bottom:150px;">
    <div class="headerDashboard">
        <div class="col-lg-11 col-md-11 col-sm-11 col-xs-11 headerText">
            <h2>राजे यशवंतराव होळकर महामेष योजना</h2>
        </div>
        <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1" style="text-align:right">
            <img src="~/Images/Raje.jpg" class="img-responsive" alt="Raje Yashwant Holkar" />
        </div>

    </div>
    <div class="panel panel-primary-2" style="box-shadow: 0 2px 2px rgba(0,0,0,.3);">
        <div class="panel-heading panel-primary-2 text-center">
            <h3 class="panel-title">Beneficiary List - Preliminary</h3>
        </div>

        <div class="panel-body">

            <div class="row text-center">
                <table class="table table-bordered table-striped table-responsive" style="width:70%;margin-left:15%;margin-top:5%">
                    <thead>
                        <tr>
                            <th>Sr. No.</th>
                            <th>District Code</th>
                            <th>District Name</th>
                            <th>Preliminary Selection Date-Time</th>
                            <th>Is Done?</th>
                            <th>Created On</th>
                            <th>File Name</th>
                        </tr>
                    </thead>
                    <tbody>
                        @for (int i = 0; i < Model.TimerList.Count; i++)
                        {
                            <tr>
                                <td>@(i + 1)</td>
                                <td>@Model.TimerList[i].DistCode</td>
                                <td>@Model.TimerList[i].DistrictName</td>
                                <td>@Model.TimerList[i].EnableDate</td>
                                <td>
                                    @if (Model.TimerList[i].IsEnabled)
                                    {<i id="check" class="fa fa-check-circle" style="color:green;font-size:25px;"></i>}
                                    else
                                    {<i id="uncheck" class="fa fa-remove" style="color:grey;font-size:25px;"></i>}
                                </td>
                                <td>@Model.TimerList[i].CreatedDate</td>
                                <td>
                                    @if (Model.TimerList[i].IsEnabled)
                                    {
                                    <a href="~/Documents/MahameshYojana/PreliminaryList_@(Model.TimerList[i].DistCode).pdf"><i class="fa fa-file-pdf-o red"></i>PreliminaryList_@Model.TimerList[i].DistCode</a>
                                    }
                                </td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script src="/Content/Varsity/assets/js/jquery.min.js" type="text/javascript"></script>

