@model Mahamesh.Models.OfficerLogin

@{
    ViewBag.Title = "Beneficiary List Preliminary & Final";
}
<style>
    .grid {
        /* Grid Fallback */
        display: flex;
        flex-wrap: wrap;
        /* Supports Grid */
        display: grid;
        grid-template-columns: repeat(auto-fill, minmax(375px, 1fr));
        grid-auto-rows: minmax(150px, auto);
        grid-gap: 1em;
    }

    .panel-title {
        font-family: Arial Unicode MS;
        font-weight: 800;
    }

    .module {
        /* Demo-Specific Styles */
        display: inherit;
        align-items: center;
        justify-content: center;
        height: 160px;
        /* Flex Fallback */
        margin-left: 5px;
        margin-right: 5px;
        flex: 1 1 200px;
        color: black !important;
        padding: 35px 10px;
        text-align: center;
    }

        .module a {
            color: black !important;
            font-weight: 600;
        }
    /* If Grid is supported, remove the margin we set for the fallback */
    @@supports (display: grid) {
        .module {
            margin: 0;
        }
    }
</style>
<div class="container body-content" id="#maincontent" style="margin-bottom:150px;">
    <div class="headerDashboard">
        <div class="col-lg-11 col-md-11 col-sm-11 col-xs-11 headerText">
            <h2>राजे यशवंतराव होळकर महामेष योजना</h2>
        </div>
        <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1" style="text-align:right">
            <img src="~/Images/Raje.jpg" class="img-responsive" alt="Raje Yashwant Holkar" />
        </div>

    </div>
    <div class="panel panel-primary-2" style="box-shadow: 0 2px 2px rgba(0,0,0,.3);">
        <div class="panel-heading panel-primary-2 text-center">
            <h3 class="panel-title">Beneficiary List Preliminary & Final</h3>
        </div>

        <div class="panel-body">
            <div class="row">

                <section id="mu-service">
                    <div class="mu-service-area">
                        <div class="grid">

                            <div class="module" style="background:#ff7d7d;">
                                <div><span style="font-size:24px;" class="fa fa-th-list"></span></div>
                                <br />
                                <div>
                                    <h5><a href="@Url.Action("PrelimBeneficiary", "Menu")">लाभार्थ्यांची प्राथमिक यादी</a></h5>
                                    <h5><a href="@Url.Action("PrelimBeneficiary", "Menu")">Beneficiary List Preliminary</a></h5>
                                </div>
                            </div>
                            <div class="module" style="background:#50d2ef">
                                <span style="font-size:24px" class="fa fa-list-ul"></span>
                                <div>
                                    <h5><a href="#">लाभार्थ्यांची अंतिम यादी </a></h5>
                                    <h5><a href="#">Beneficiary List Final</a></h5>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>

            
        </div>
    </div>
</div>
<script src="/Content/Varsity/assets/js/jquery.min.js" type="text/javascript"></script>

