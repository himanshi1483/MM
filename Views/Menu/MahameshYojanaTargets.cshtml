<script src="/Content/Varsity/assets/js/jquery.min.js" type="text/javascript"></script>
<script>
    $(document).ready(function () {

        var Component = "@Model.Component";
       
        if (Component != "") {
            $("#Component").val(Component);
        }

          var District = "@Model.DistrictName";
        if (District != "") {
            $("#DistrictName").val(District);
        }

        var Taluka = "@Model.TalukaName";
       // alert(Taluka);
        if (Taluka != "") {
            $("#TalukaName").val(Taluka);
        }
       
    });
</script>
<script>
 
           $("#DistrictName").change(function () {
                $("#TalukaName").empty();
                $.ajax({
                  type: 'POST',
                    url: '@Url.Action("getTaluka")',
                    dataType: 'json',
                    data: { dist: $("#DistrictName").val() },
                    success: function(city) {
                        //alert(city);
                        $.each(city, function(i, city) {
                            $("#TalukaName").append('<option value="'
                                + city.Value + '">'
                                + city.Text + '</option>');
                });
            },
                    error: function(ex) {
                alert('Failed.' + ex);
            }
        });
        return false;
});


</script>
