            @using (Html.BeginForm("MahameshYojanaTargets", "Menu", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <div class="row">
                    <div class="form-group col-lg-6">
                        <label for="Comp" class="col-sm-2 control-label lbl">घटक:</label>
                        <select class="form-control col-sm-10" style="width:75%" name="Component" id="Component">
                            <option value="0">
                                ----
                            </option>
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                       
                        </select>

                    </div>
                    <div class=" form-group col-lg-6">
                        <label for="district" class="col-sm-2 control-label lbl">जिल्हा:</label>
                        <div class="col-sm-10">
                            @Html.DropDownListFor(x => x.DistrictName, (IEnumerable<SelectListItem>)ViewBag.District, new { @class = "form-control" })

                        </div>
                    </div>
                </div>
                <div class="row">

                    <div class="form-group col-xs-6">
                        <label for="Comp" class="col-sm-2 control-label lbl">तालुका:</label>
                        <div class="col-sm-10" style="width:75%">
                            @Html.DropDownListFor(x => x.TalukaName, (SelectList)ViewBag.Taluka,"Select", new { @class = "form-control" })

                        </div>
                    </div>
                </div>
                <div class="row text-center">
                    <button class="btn btn-primary" id="btnShow" type="submit">Show</button>
                </div>
            }
