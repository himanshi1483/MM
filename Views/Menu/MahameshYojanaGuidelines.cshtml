
@{
    ViewBag.Title = "Govt. Guidelines";
}
<link href="/Content/Varsity/ui.css" rel="stylesheet">

<div class="container body-content" id="#maincontent" style="margin-bottom: 20px;">
    <div class="headerDashboard">
        <div class="col-lg-11 col-md-11 col-sm-11 col-xs-11 headerText">
            <h2>राजे यशवंतराव होळकर महामेष योजना</h2>
        </div>
        <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1" style="text-align:right">
            <img src="~/Images/Raje.jpg" class="img-responsive" alt="Raje Yashwant Holkar" />
        </div>

    </div>
    <div class="panel panel-primary-2" style="box-shadow: 0 2px 2px rgba(0,0,0,.3);">

        <div class="panel-body">
            <div class="row">
                <div class="heading">
                    <h5 class="text-center">Government Guidelines</h5>
                </div>
                <article class="box">
                    <figure class="itemside">
                        <div class="aside align-self-center">
                            <span class="icon-wrap icon-md round bg-danger-1">
                                <i class="fa fa-file-pdf-o white"></i>
                            </span>
                        </div>
                        <figcaption class="text-wrap">
                            <h5 class="title">
                                <a target="_blank" accesskey=" " href="~/Documents/MahameshYojana/Govt GR.pdf">Govt GR.pdf</a>
                            </h5>
                        </figcaption>
                    </figure> <!-- iconbox // -->
                </article> <!-- panel-lg.// -->
                <article class="box">
                    <figure class="itemside">
                        <div class="aside align-self-center">
                            <span class="icon-wrap icon-md round bg-warning-1">
                                <i class="fa fa-file-pdf-o white"></i>
                            </span>
                        </div>
                        <figcaption class="text-wrap">
                            <h5 class="title">
                                <a target="_blank" accesskey=" " href="~/Documents/MahameshYojana/MAHAMESH GUIDELINE 2019-20.pdf">Mahamesh Guidelines 2019 -20.pdf</a>
                            </h5>
                        </figcaption>
                    </figure> <!-- iconbox // -->
                </article> <!-- panel-lg.// -->
            </div><!-- col // -->
        </div>
    </div>
</div>
