@model Mahamesh.Models.OfficerLogin

@{
      ViewBag.Title = "Selected List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" rel="stylesheet" />
<link href="https://cdn.datatables.net/buttons/1.5.6/css/buttons.dataTables.min.css" rel="stylesheet" />
<style>
    .tableheader {
        background: #1a9b83 !important;
        color: white;
        text-align: center;
        font-weight: bold;
    }

    .table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
        padding: 5.5px;
        /* font-size: 13px; */
        line-height: 1.42857143;
        vertical-align: top;
        border-top: 1px solid #ddd;
    }
</style>
<div class="container-fluid" style="margin-bottom:150px;">
    <div class="headerDashboard">
        <div class="col-lg-11 col-md-11 col-sm-11 col-xs-11 headerText">
            <h2>राजे यशवंतराव होळकर महामेष योजना</h2>
        </div>
        <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1" style="text-align:right">
            <img src="~/Images/Raje.jpg" style="max-height:93px" class="img-responsive" alt="Raje Yashwant Holkar" />
        </div>

    </div>
    <div class="panel panel-primary-2">
        @*<p>
                @Html.ActionLink("Create New", "Create")
            </p>*@
        <div class="panel-heading panel-primary-2 text-center"><h4>Applicant's List</h4></div>
        <div class="panel-body">
            <h4> Selected List for Handicapped</h4>
            <table class="display table table-bordered table-striped3 table-responsive" id="table1">
                <thead>
                    <tr>
                        <th>Sr. No.</th>
                        <th>Application Number</th>
                        <th>Component</th>
                        <th>District</th>
                        <th>Taluka</th>
                        <th>Village</th>
                        <th>Aadhar Number</th>
                        <th>Name</th>
                        <th>Phone No.</th>
                        <th>Gender</th>
                        <th>Handicapped?</th>
                        <th>Selected Number</th>

                    </tr>
                </thead>
                <tbody>
                    @for (int i = 0; i < Model.SelectedList.SelectedHandicappedList.Count; i++)
                    {
                        <tr>
                            <td>@(i + 1)</td>
                            <td>@Model.SelectedList.SelectedHandicappedList[i].ApplicationNumber</td>
                            <td>@Model.SelectedList.SelectedHandicappedList[i].Component</td>
                            <td>@Model.SelectedList.SelectedHandicappedList[i].District_Mr.ToString()</td>
                            <td>@Model.SelectedList.SelectedHandicappedList[i].Taluka_Mr</td>
                            <td>@Model.SelectedList.SelectedHandicappedList[i].VillageName</td>
                            <td>@Model.SelectedList.SelectedHandicappedList[i].AadharNo</td>
                            <td>@Model.SelectedList.SelectedHandicappedList[i].Name</td>
                            <td>@Model.SelectedList.SelectedHandicappedList[i].PhNo</td>
                            <td>@Model.SelectedList.SelectedHandicappedList[i].Gender</td>
                            <td>@Model.SelectedList.SelectedHandicappedList[i].ApplicantCrippled</td>
                            <td>SL_@Model.SelectedList.SelectedHandicappedList[i].Component _@(i + 1)</td>
                        </tr>
                    }
                </tbody>
            </table>
            <br />
            <h4> Selected List for Females</h4>
            <table class="display table table-bordered table-striped3 table-responsive" id="table2">
                <thead>
                    <tr>
                        <th>Sr. No.</th>
                        <th>Application Number</th>
                        <th>Component</th>
                        <th>District</th>
                        <th>Taluka</th>
                        <th>Village</th>
                        <th>Aadhar Number</th>
                        <th>Name</th>
                        <th>Phone No.</th>
                        <th>Gender</th>
                        <th>Handicapped?</th>
                        <th>Selected Number</th>
                    </tr>
                </thead>
                <tbody>
                    @for (int i = 0; i < Model.SelectedList.SelectedFemaleList.Count; i++)
                    {
                        <tr>
                            <td>@(i + 1)</td>
                            <td>@Model.SelectedList.SelectedFemaleList[i].ApplicationNumber</td>
                            <td>@Model.SelectedList.SelectedFemaleList[i].Component</td>
                            <td>@Model.SelectedList.SelectedFemaleList[i].District_Mr</td>
                            <td>@Model.SelectedList.SelectedFemaleList[i].Taluka_Mr</td>
                            <td>@Model.SelectedList.SelectedFemaleList[i].VillageName</td>
                            <td>@Model.SelectedList.SelectedFemaleList[i].AadharNo</td>
                            <td>@Model.SelectedList.SelectedFemaleList[i].Name</td>
                            <td>@Model.SelectedList.SelectedFemaleList[i].PhNo</td>
                            <td>@Model.SelectedList.SelectedFemaleList[i].Gender</td>
                            <td>@Model.SelectedList.SelectedFemaleList[i].ApplicantCrippled</td>
                            <td>SL_@Model.SelectedList.SelectedFemaleList[i].Component _@(i + 1)</td>
                        </tr>
                    }
                </tbody>
            </table>
            <br />
            <h4> Selected List for General Target</h4>
            <table class="display table table-bordered table-striped3 table-responsive" id="table3">
                <thead>
                    <tr>
                        <th>Sr. No.</th>
                        <th>Application Number</th>
                        <th>Component</th>
                        <th>District</th>
                        <th>Taluka</th>
                        <th>Village</th>
                        <th>Aadhar Number</th>
                        <th>Name</th>
                        <th>Phone No.</th>
                        <th>Gender</th>
                        <th>Handicapped?</th>
                        <th>Selected Number</th>
                    </tr>
                </thead>
                <tbody>
                    @for (int i = 0; i < Model.SelectedList.SelectedGeneralList.Count; i++)
                    {
                        <tr>
                            <td>@(i + 1)</td>
                            <td>@Model.SelectedList.SelectedGeneralList[i].ApplicationNumber</td>
                            <td>@Model.SelectedList.SelectedGeneralList[i].Component</td>
                            <td>@Model.SelectedList.SelectedGeneralList[i].District_Mr</td>
                            <td>@Model.SelectedList.SelectedGeneralList[i].Taluka_Mr</td>
                            <td>@Model.SelectedList.SelectedGeneralList[i].VillageName</td>
                            <td>@Model.SelectedList.SelectedGeneralList[i].AadharNo</td>
                            <td>@Model.SelectedList.SelectedGeneralList[i].Name</td>
                            <td>@Model.SelectedList.SelectedGeneralList[i].PhNo</td>
                            <td>@Model.SelectedList.SelectedGeneralList[i].Gender</td>
                            <td>@Model.SelectedList.SelectedGeneralList[i].ApplicantCrippled</td>
                            <td>SL_@Model.SelectedList.SelectedGeneralList[i].Component _@(i + 1)</td>
                        </tr>
                    }
                </tbody>
            </table>
            <br />

            <h4> Waiting List</h4>
            <table class="display table table-bordered table-striped table-responsive" id="table4">
                <thead>
                    <tr>
                        <th>Sr. No.</th>
                        <th>Application Number</th>
                        <th>Component</th>
                        <td style="display:none"></td>
                        <th>District</th>
                        <th>Taluka</th>
                        <th>Village</th>
                        <th>Aadhar Number</th>
                        <th>Name</th>
                        <th>Phone No.</th>
                        <th>Gender</th>
                        <th>Handicapped?</th>
                        <th>Selected Number</th>
                    </tr>
                </thead>
                <tbody>
                    @for (int i = 0; i < Model.SelectedList.WaitingList.Count; i++)
                    {
                        <tr>
                            <td>@(i + 1)</td>
                            <td>@Model.SelectedList.WaitingList[i].ApplicationNumber</td>
                            <td>@Model.SelectedList.WaitingList[i].Component</td>
                            <td style="display:none" class="distCode">@Model.SelectedList.WaitingList[i].DistCode</td>
                            <td>@Model.SelectedList.WaitingList[i].District_Mr</td>
                            <td>@Model.SelectedList.WaitingList[i].Taluka_Mr</td>
                            <td>@Model.SelectedList.WaitingList[i].VillageName</td>
                            <td>@Model.SelectedList.WaitingList[i].AadharNo</td>
                            <td>@Model.SelectedList.WaitingList[i].Name</td>
                            <td>@Model.SelectedList.WaitingList[i].PhNo</td>
                            <td>@Model.SelectedList.WaitingList[i].Gender</td>
                            <td>@Model.SelectedList.WaitingList[i].ApplicantCrippled</td>
                            <td>WL_@Model.SelectedList.WaitingList[i].Component _@(i + 1)</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
<script src="/Content/Varsity/assets/js/jquery.min.js" type="text/javascript"></script>
<script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js" type="text/javascript"></script>
<script src="https://cdn.datatables.net/buttons/1.5.6/js/dataTables.buttons.min.js" type="text/javascript"></script>
<script src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.flash.min.js" type="text/javascript"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js" type="text/javascript"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.32/pdfmake.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js" type="text/javascript"></script>
<script src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.html5.min.js" type="text/javascript"></script>
<script src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.print.min.js" type="text/javascript"></script>
@*<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.32/vfs_fonts.js"></script>*@
<script src="~/Content/vfs-fonts.js"></script>
<script>
      pdfMake.fonts = {
       'Lohit-Devanagari': {
                normal: 'gargi.ttf',
                bold: 'gargi.ttf',
                italics: 'gargi.ttf',
                bolditalics: 'gargi.ttf'
        }
};
        $('#table1').DataTable
            ({
                dom: 'Bfrtip',
                buttons: [
                    { extend: 'copyHtml5', title: 'PreliminaryList_'+@Model.district+'_Handicap' },
                    { extend: 'excelHtml5', title: 'PreliminaryList_'+@Model.district+'_Handicap' },
                    { extend: 'csvHtml5', title: 'PreliminaryList_'+@Model.district+'_Handicap' },
                    { extend: 'print', title: 'PreliminaryList_' +@Model.district+'_Handicap' },
                    { extend: 'pdfHtml5', orientation: 'landscape', title: 'PreliminaryList_' +@Model.district+'_Handicap', pageSize: 'A4',exportOptions: {columns: ':visible'},
                                customize: function (doc) {
                            doc.defaultStyle.font = 'Lohit-Devanagari';
                        }
                    }
                ],
                "paging": false,
                 "ordering": false,
                "info" : false,
                "searching" : false
            });
        $('#table2').DataTable
            ({
                dom: 'Bfrtip',
                buttons: [
                    { extend: 'copyHtml5', title: 'PreliminaryList_'+@Model.district+'_Female' },
