                    { extend: 'copyHtml5',title: 'PreliminaryList_'+@Model.district+'_General' },
                    { extend: 'excelHtml5', title: 'PreliminaryList_'+@Model.district+'_General'  },
                    { extend: 'csvHtml5',title: 'PreliminaryList_'+@Model.district+'_General'  },
                    { extend: 'print', title: 'PreliminaryList_'+@Model.district+'_General'  },
                    { extend: 'pdfHtml5', orientation: 'landscape', title: 'PreliminaryList_' +@Model.district+'_General', pageSize: 'A4',exportOptions: {columns: ':visible'},
                                customize: function (doc) {
                            doc.defaultStyle.font = 'Lohit-Devanagari';
                        }
                    }
                ],
                  "paging": false,
                 "ordering": false,
                "info" : false,
                "searching" : false
            });
        $('#table4').DataTable
            ({
