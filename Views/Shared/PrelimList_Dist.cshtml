                                customize: function (doc) {
                            doc.defaultStyle.font = 'Lohit-Devanagari';
                        }
                    }
                ],
                 "paging": false,
                 "ordering": false,
                "info" : false,
                "searching" : false
            });
        $('#table3').DataTable
            ({
                dom: 'Bfrtip',
