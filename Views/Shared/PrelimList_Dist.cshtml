                    { extend: 'copyHtml5', title: 'PreliminaryList_'+@Model.district+'_Waiting' },
                    { extend: 'excelHtml5',title: 'PreliminaryList_'+@Model.district+'_Waiting'  },
                    { extend: 'csvHtml5',title: 'PreliminaryList_'+@Model.district+'_Waiting'  },
                    { extend: 'print', title: 'PreliminaryList_'+@Model.district+'_Waiting' },
                    { extend: 'pdfHtml5', orientation: 'landscape', title: 'PreliminaryList_' +@Model.district+'_Waiting', pageSize: 'A4',exportOptions: {columns: ':visible'},
                                customize: function (doc) {
