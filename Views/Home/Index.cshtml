<style>
    .figure {
        display: inline-block;
    }

    .figure-img {
        margin-bottom: 0.5rem;
        line-height: 1;
    }

    .figure-caption {
        font-size: 110%;
        color: #6c757d;
    }

    .carousel {
        padding: -1px 380px;
    }

    .carousel__button--prev, .carousel__button--next {
        position: absolute;
        top: 62px;
        /*bottom: 0;*/
    }

    .carousel__button--prev {
        left: 2%;
    }

    .carousel__button--next {
        right: 2%;
    }

    .carousel__container {
        overflow: hidden;
        height: 150px;
    }

    .carousel__list {
        position: relative;
        display: flex;
        flex-wrap: wrap;
    }

        .carousel__list.unwrap {
            flex-wrap: nowrap;
        }

    .carousel__item {
        flex: 1 0 250px;
        /* 	max-width: 300px; */
        height: 200px;
        background-color: transparent;
        text-align: center;
        line-height: 100px;
        /*border: 1px solid #fff;*/
    }

    .img-border {
        border: 1px solid black;
    }

    .video-container {
        position: relative;
        padding-bottom: 56.25%;
        height: 0;
        overflow: hidden;
    }

        .video-container video,
        .video-container object,
        .video-container embed {
            position: absolute;
            top: 0;
            left: 10px;
            padding-right: 21px;
            width: 100%;
            height: auto;
        }
</style>

